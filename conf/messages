#Localize Default Messages

# --- Constraints
constraint.required=Erforderlich
constraint.min=Minimum value: {0}
constraint.max=Maximum value: {0}
constraint.minLength=Minimale Länge: {0}
constraint.maxLength=Maximale Länge: {0}
constraint.email=Email

# --- Formats
format.date=dd.MM.yy
format.numeric=Numeric
format.real=Real

# --- Errors
error.invalid=Ungültiger Wert
error.required=Das Feld darf nicht leer sein
error.number=Nur Zahlen erlaubt
error.real=Nur eine Kommazahlen erlaubt
error.real.precision=Nur Kommazahlen mit nicht mehr als {0} Ziffern mit {1} Nachkommastellen erlaubt
error.min=Muss größer oder gleich sein wie {0}
error.min.strict=Muss größter sein als {0}
error.max=Muss kleiner oder gleich sein wie {0}
error.max.strict=Muss kleiner sein als {0}
error.minLength=Minimale Länge ist {0}
error.maxLength=Maximale Länge ist {0}
error.email=Email ist ungültig
error.pattern=Muss folgende Regel erfüllen: {0}

